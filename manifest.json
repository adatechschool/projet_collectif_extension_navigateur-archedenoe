{
    "manifest_version": 3,
    "name": "Create Extension",
    "version": "1.0",
    "content_scripts": 
    [
        {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
        }
    ],
    "web_accessible_resources": [{
        "resources": ["images/*.png", "images/*.jpg"],
        
        "matches": ["<all_urls>"]
      }]
}